									"pm.test(\"Status code é 200, 201 ou 204\", function () {",
									"    pm.expect(pm.response.code).to.be.oneOf([200, 201, 204]);",
									"});",
									"",
									"if (pm.response.code === 204 || pm.response.text() === '') {",
									"    console.log('API retornou sem body - buscando ID na listagem...');",
									"    ",
									"    pm.sendRequest({",
									"        url: pm.environment.get('baseUrl') + '/api/v1/FilialApi',",
									"        method: 'GET',",
									"        header: {",
									"            'Authorization': 'Bearer ' + pm.environment.get('authToken')",
									"        }",
									"    }, function (err, response) {",
									"        if (!err) {",
									"            var filiais = response.json();",
									"            var filialCriada = filiais.find(f => f.nomeFilial === 'Filial Teste Postman');",
									"            if (filialCriada) {",
									"                pm.environment.set('novaFilialId', filialCriada.idFilial);",
									"                console.log('ID encontrado:', filialCriada.idFilial);",
									"            }",
									"        }",
									"    });",
									"} else {",
									"    try {",
									"        var jsonData = pm.response.json();",
									"        var id = jsonData.idFilial || jsonData.id;",
									"        if (id) {",
									"            pm.environment.set('novaFilialId', id);",
									"            console.log('ID salvo:', id);",
									"        }",
									"    } catch (e) {",
									"        console.log('Erro ao parsear JSON:', e);",
									"    }",
